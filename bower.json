  "keywords": ["capital-framework", "capital", "tabs", "less"],
  "authors": [
    {
      "name": "Consumer Financial Protection Bureau",
      "url": "http://cfpb.github.io/"
    }
  ],
  "homepage": "https://github.com/cfpb/cf-tabs",
  "repository": {
    "type": "git",
    "url": "https://github.com/cfpb/cf-tabs.git"
  },
  "license": "https://github.com/cfpb/cf-tabs/blob/master/TERMS.md",
  "main": "src/",
  "dependencies": {
    "jquery": "~1.11",
    "cf-colors": "git://github.com/cfpb/cf-colors.git",
    "cf-typography": "git://github.com/cfpb/cf-typography.git"
  },
  "exportsOverride": {
    "cf-*": {
      "css": "src/**/*.css",
